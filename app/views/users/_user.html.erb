<div id="<%= dom_id user %>">
  <p>
    <strong>Name:</strong>
    <%= user.name %>
  </p>

  <p>
    <strong>Cpf:</strong>
    <%= user.cpf %>
  </p>

  <p>
    <strong>Email:</strong>
    <%= user.email %>
  </p>

  <p>
    <strong>Role:</strong>
    <%= user.role %>
  </p>

  <p>
    <strong>State:</strong>
    <%= user.state %>
  </p>

  <p>
    <strong>Cep:</strong>
    <%= user.cep %>
  </p>

  <p>
    <strong>Street:</strong>
    <%= user.street %>
  </p>

  <p>
    <strong>Number:</strong>
    <%= user.number %>
  </p>

</div>
